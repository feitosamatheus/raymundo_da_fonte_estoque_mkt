<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Estoque.MVC</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/icons/font/bootstrap-icons.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/layout/index.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/custom-style.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Estoque.MVC.styles.css" asp-append-version="true" />
</head>
<body>
    @await Html.PartialAsync("_HeaderVazio")
    <section class="cont-section-principal">
        <main class="main" role="main">
            <section class="pb-3">
                @RenderBody()
            </section>
        </main>
    </section>

    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
